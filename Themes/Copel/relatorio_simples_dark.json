                    }],

                "border": [{ 
                    "show": false 
                    }],

                "visualHeader": [{
                    "foreground": {"solid": { "color": "#C46C15"}},
                    "transparency": 100
                    }],

                "outspacePane": [{
                    "checkboxAndApplyColor": {
                        "solid": {"color": "#C46C15"}
                        }
                    }]
            }
        },
        "page": {
            "*": {
                "background": [{
                        "color": { "solid": { "color": "#151514"}},
                        "transparency": 100
                    }],
              
                "outspace": [{
                      "color": {"solid": { "color": "#4E4D4D" } }
                    }]
            }
